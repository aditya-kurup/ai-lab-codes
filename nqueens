def is_safe(board, row, col, n):
    # Check if a queen can be placed safely at board[row][col]
    for i in range(col):  # Check same row
        if board[row][i] == 1:
            return False

    # Check upper-left diagonal
    for i, j in zip(range(row, -1, -1), range(col, -1, -1)):
        if board[i][j] == 1:
            return False

    # Check lower-left diagonal
    for i, j in zip(range(row, n), range(col, -1, -1)):
        if board[i][j] == 1:
            return False

    return True  # Safe spot


def solve(board, col, n):
    if col == n:  # All queens placed
        print_board(board)
        return True

    for row in range(n):
        if is_safe(board, row, col, n):
            board[row][col] = 1  # Place queen
            if solve(board, col + 1, n):  # Move to next column
                return True
            board[row][col] = 0  # Remove queen (backtrack)

    return False  # No valid place found


def print_board(board):
    for row in board:
        print(row)
    print()  # Blank line between solutions


# ðŸ§  Try for N = 4
n = 4
board = [[0] * n for _ in range(n)]
solve(board, 0, n)
